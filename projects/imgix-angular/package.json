{
  "name": "@imgix/angular",
  "version": "0.0.0-semantically-released",
  "license": "BSD-2-Clause",
  "peerDependencies": {},
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "imgix-core-js": "2.3.1",
    "imgix-url-params": "^11.9.1",
    "tslib": "^2.0.2",
    "urijs": "1.19.2"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": "rc"
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@google/semantic-release-replace-plugin",
        {
          "replacements": [
            {
              "files": [
                "src/common/constants.ts"
              ],
              "from": "IMGIX_NG_VERSION: string = '.*'",
              "to": "IMGIX_NG_VERSION: string = '${nextRelease.version}'",
              "results": [
                {
                  "file": "src/common/constants.ts",
                  "hasChanged": true,
                  "numMatches": 1,
                  "numReplacements": 1
                }
              ],
              "countMatches": true
            }
          ]
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./dist"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "cp ../../README.md . && ../../node_modules/.bin/ng build imgix-angular"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "src/**",
            "dist/**",
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes} [skip ci]"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist/bundles/imgix-angular.umd.min.js",
              "label": "Standalone browser build"
            }
          ]
        }
      ]
    ]
  }
}
